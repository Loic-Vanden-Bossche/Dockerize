{
  "name": "test-proj",
  "version": "0.0.1",
  "description": "My awesome node server",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "lint": "npx eslint {src,test}/**/*.ts --fix",
    "db:seed": "TYPEORM_ENTITIES='src/**/*.model.ts' npx ts-node ./scripts/seeds",
    "format": "npx prettier --write {src,test}/**/*.ts",
    "migration:generate": "TYPEORM_MIGRATIONS_DIR='scripts/migrations' TYPEORM_ENTITIES='src/**/*.model.ts' npm run typeorm migration:generate",
    "migration:run": "TYPEORM_MIGRATIONS='scripts/migrations/*.ts' npm run typeorm migration:run",
    "typeorm": "npx ts-node ./node_modules/typeorm/cli.js",
    "test": "NODE_ENV=test npx jest --runInBand --config jest.config.json --verbose",
    "test:e2e": "jest --config test/jest-e2e.json --runInBand",
    "test:watch": "NODE_ENV=test npx jest --runInBand --config jest.config.json --watch",
    "test:cov": "NODE_ENV=test npx jest --runInBand --config jest.config.json --coverage",
    "test:api": "npx ts-node ./scripts/ci/run-collection.ts",
    "test:debug": "NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand  --config jest.config.json",
    "build": "npx nest build",
    "start": "nodemon src/main.ts",
    "start:debug": "npx nest start --debug --watch",
    "start:prod": "NODE_ENV=prod node dist/main",
    "start:dev": "nest start --watch"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/common": "^7.6.15",
    "@nestjs/core": "^7.6.15",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/swagger": "^4.8.2",
    "@nestjs/typeorm": "^7.0.0",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "inquirer": "^8.0.0",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.4.1",
    "passport-http-bearer": "^1.0.1",
    "pg": "^8.7.1",
    "pino": "^6.0.0",
    "pino-pretty": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "^0.2.24"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.7",
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.6",
    "@types/faker": "^5.5.3",
    "@types/inquirer": "^7.3.1",
    "@types/jest": "^26.0.22",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/node": "^14.14.41",
    "@types/passport": "^1.0.3",
    "@types/passport-http-bearer": "^1.0.34",
    "@types/passport-local": "^1.0.33",
    "@types/pino": "^6.0.0",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "faker": "^5.5.3",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.0.7",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0",
    "prettier": "^2.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.5",
    "ts-loader": "^9.0.2",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "all": true
  }
}
